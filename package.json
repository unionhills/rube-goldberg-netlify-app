{
  "name": "rube-goldberg-app",
  "version": "0.1.0",
  "scripts": {
    "postinstall": "cd functions/api && npm install",
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "rimraf dist",
    "build": "npm run build:ui && npm run build:functions",
    "postbuild": "netlify-lambda install",
    "test": "npm run test:ui && npm run test:functions",
    "test:ui": "ng test",
    "test:functions": "cd functions/api && npm test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ui": "ng build",
    "build:functions": "tsc -p tsconfig.functions.json && copyfiles \"functions/*/package*.json\" dist",
    "format": "prettier --write \"{src,functions}/**/*.ts\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.9",
    "@angular/common": "~10.0.9",
    "@angular/compiler": "~10.0.9",
    "@angular/core": "~10.0.9",
    "@angular/forms": "~10.0.9",
    "@angular/platform-browser": "~10.0.9",
    "@angular/platform-browser-dynamic": "~10.0.9",
    "@angular/router": "~10.0.9",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "bootstrap": "^4.5.2",
    "mongoose": "^5.9.15",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.6",
    "@angular/cli": "~10.0.6",
    "@angular/compiler-cli": "~10.0.9",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/mongoose": "^5.7.21",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "netlify-lambda": "^1.6.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  },
  "optionalDependencies": {
    "prettier": "^1.19.1"
  }
}
